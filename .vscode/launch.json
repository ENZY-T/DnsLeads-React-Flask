	"version": "0.2.0",
	"configurations": [
		{
			"name": "DnsLeads: Flask",
			"type": "python",
			"request": "launch",
			"module": "flask",
			"cwd": "${workspaceFolder}/backend",
			"env": {
				"FLASK_APP": "app.py",
				"FLASK_DEBUG": "1"
			},
			"args": ["run", "--no-debugger", "--no-reload"],
			"jinja": true,
			"justMyCode": true
		}
	]
}
